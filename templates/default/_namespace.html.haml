%h2
  = namespace.html_qualified_name

= partial "_description.html.haml", { describable: namespace }

- if namespace.has_namespaces?
  = partial '_table.html.haml', { heading: "Namespaces", items: namespace.namespaces }

- if namespace.has_classes?
  = partial '_table.html.haml', { heading: "Classes", items: namespace.classes }

